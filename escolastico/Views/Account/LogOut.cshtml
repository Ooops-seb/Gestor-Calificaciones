@{
    Layout = "_Layout";
    ViewData["Title"] = "Sesión Cerrada";
}

<div class="flex-c center color-white txt-center">
    <div class="mg-20 color-brown border-n-r">
        <h2 class="txt-white">@ViewData["Title"]</h2>
        <div class="apply-request flex-c center pad-10 color-white">
            <p class="txt-justify fz-16">
                Su sesión ha sido cerrada.
            </p>
            <a asp-controller="Account" asp-action="Login" class="btn btn-prop color-yellow txt-white pad-10">Regresar al Inicio</a>
        </div>
    </div>
</div>