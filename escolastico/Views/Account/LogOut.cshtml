
@{
    ViewData["Title"] = "Sesión Cerrada";
}

<div class="flex-column centering color-white txt-center">
    <div class="margin-a-20 color-brown border-n-r">
        <h2 class="txt-white">@ViewData["Title"]</h2>
        <div class="apply-request flex-column centering padding-a-10 color-white">
            <p class="txt-justify txt-f16px">
                Su sesión ha sido cerrada.
            </p>
            <a asp-controller="Account" asp-action="Login" class="btn btn-prop color-yellow txt-white padding-a-10">Regresar al Inicio</a>
        </div>
    </div>
</div>